---

<div class="main-container">

  <link href="https://d1fto35gcfffzn.cloudfront.net/docs/styleguide-v13-app.css" type="text/css" rel="stylesheet"/>
  <style>
  body {
    overflow-y: inherit;
    background-color: #f8f8f8;
  }
  a, .link {
    border-bottom: none;
  }
  .to-top a,
  a#top {
    display: none;
  }
  #container-main.container {
    max-width: 1260px;
  }
  .sidebar {
    display: none;
  }
  ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
    color: rgba(0,0,0,0.8);
  }
  ::-moz-placeholder { /* Firefox 19+ */
    color: rgba(0,0,0,0.8);
  }
  :-ms-input-placeholder { /* IE 10+ */
    color: rgba(0,0,0,0.8);
  }
  :-moz-placeholder { /* Firefox 18- */
    color: rgba(0,0,0,0.8);
  }
  form#search-home {
    margin: 0 auto 60px;
    max-width: 760px;
  }
  form#search-home input {
    height: 50px;
    background-image: url(https://d1fto35gcfffzn.cloudfront.net/images/docs/md-search.svg);
    background-repeat: no-repeat;
    background-position: 10px 12px;
    font-size: 18px;
    padding-left: 40px;
  }
  #pcf-landing {
    margin-top: 40px;
  }
  @media(max-width: 699px) {
    #pcf-landing {
      margin-top: 0;
    }
  }
  .container {
    background-color: #f8f8f8;
    max-width: none;
  }
  .external-link:after {
    content: none;
  }
  .docs-home-panel  {
    background-color: white;
    display: flex;
    flex-direction: column;

    box-shadow: none;
    margin: 0;
  }
  .docs-home-panel .logo-icon img {
    max-height: 4.8em;
  }
  .content a[href^="http"]:after {
    display: none;
  }
  .content a:focus,
  .content a:active,
  .content a:hover {
    border-bottom-style: none;
  }
  .content a:focus,
  .content a:active {
    background-color: transparent;
  }
  .content a:focus {
    text-decoration: none;
  }
  .container img {
    max-width: 6.4em;
    width: 100%;
  }
  .container .grid a.docs-home-panel[href*='communityedition'] img {max-width:10em}
  .docs-link-panel {
    height: 64px;
  }
  .docs-link-panel {
    height: 64px;
  }
  .docs-link-panel:hover  {
    border-bottom: none;
  }
  .docs-link-panel > div {
    margin: 0 auto;
  }
  .content h6,
  .content .h6 {
    font-size: .9em;
  }
  .content .type-brand-8 {
    color: #314351 !important;
  }
  .content .type-accent-5 {
    color: #0065d5 !important;
  }

  @media(max-width:1200px) {
    #container-main.container {
     max-width: 960px;
    }
  }

  @media(max-width:768px) {
    #container-main.container {
     max-width: 500px;
    }
  }

  a, .link, .quick-links a {
  color: #1D428A;
  border-bottom: none;
  font-weight: 600;
  }

  .btn.btn-default {
  background-color: #1D428A;
  border-color: #1D428A;
  }

  @media(max-width:993px) {
    body[class*=__index] header.global-header .pivotal-logo {
    max-width: 233px;
    }
  }

  @media (max-width: 993px) {
    header.global-header #info-search-bar {
    min-height: 0;
    }
  }
  </style>


  <p class="note">This page, and all other pages hosted at the docs.pivotal.io domain, will go offline on December 31, 2023. Use the links at <a href="https://docs.vmware.com/allproducts.html">https://docs.vmware.com/allproducts.html</a> to find the most up-to-date VMware Tanzu product documentation.</p>

  <!--
  <form method="get" id='search-home' action="/search">
    <input name="q" placeholder="Search docs" class="search-box" />
  </form>
  -->

  <div id="pcf-landing" class="main">



    <%# -------------- FEATURED SERVICE ------------- %>

    <% current_page.data.featured_service do |featured_service| %>
      <div class="docs-home-panel box-shadow-1 border-rounded mbxl grid paxl">
        <div class="col">
          <div class="grid">
            <div class="col col-2 col-md"><img src="images/<%= featured_service.logo ? featured_service.logo : current_page.data.default_logo %>" height="64" alt="<%= featured_service.name %> logo"></div>
            <div class="col col-md pvxl">
              <div class="configure">
                <div class="prodname">
                  <h2><%= featured_service.name %></h2>
                  <p class="type-dark-4 h6"><%= featured_service.desc %></p><br />
                  <a href="<%= featured_service.button.url %>" class="btn btn-default" type="button" aria-label="Default">
                    <span class="btn-inner-content">
                      <%= featured_service.button.name.upcase %>
                    </span>
                  </a>
                </div>
              </div>
            </div>
            <div class="col col-8 col-sm">
              <% featured_service.links.each do |link|  %>
                <h6><%= link.open_source ? "Open Source: " : "" %><a href="<%= link.url %>"><%= link.name %></a></h6>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    <% end %>




    <%# ---------- TOP SERVICES ------------ %>

    <%# Record starting point so that row can reset when you configure a different
    number of items in a row. %>
    <% @starting_point = 0 %>

    <div class="grid">
    <% current_page.data.top_services.each_with_index do |top_service, index| %>

    <%# Default number of items in a row is 3. Can be changed in Frontmatter. %>
    <% @num_in_row = top_service.num_in_row? ? top_service.num_in_row : 3 %>


    <div class="docs-home-panel pvxl phxl box-shadow-1 border-rounded col mbxl mhl">
      <div class="grid">
        <div class="col col-4"><img src="images/<%= top_service.logo ? top_service.logo : current_page.data.default_logo %>" height="64" alt="<%= top_service.name %> logo"></div>
        <div class="col">
          <h4><%= top_service.name %></h4>
          <h6 class="type-dark-4"><%= top_service.desc %></h6>
          <% if top_service.links? %>
            <% top_service.links.each do |link| %>
              <h6><%= link.open_source? ? "Open Source: " : "" %><a href="<%= link.url %>"><%= link.name %></a></h6>
            <% end %>
          <% end %>
        </div>
      </div>
    </div>
    <% if (index - @starting_point) % @num_in_row == (@num_in_row - 1) and index != 0 and index < current_page.data.top_services.length - 1 %>
      <% @starting_point = index + 1 %>
      </div>
      <div class="grid">
    <% end %>
    <% end %>
    </div>




    <%# ------------ ADD-ON SECTIONS ------------- %>

    <% current_page.data.addon do |addon| %>

    <br />
    <h2 id="add-ons"><%= addon.name %></h2>

    <% addon.addon_sections.each do |addon_section| %>
    <br />
    <div class="grid">
      <div class="col col-8 col-sm col-lg pvxl">
        <h3><%= addon_section.name  %></h3>
        <h6><%= addon_section.desc %></h6></div>
          <div class="col">
            <div class="grid">
              <% addon_section.addon_services.each do |addon_service| %>
              <div class="col col-12 col-sm">
                <a class="docs-home-panel box-shadow-1 border-rounded col mbxl" href="<%= addon_service.url %>">
                  <div class="grid docs-link-panel">
                    <div class="col col-4 col-middle"><img src="images/<%= addon_service.logo ? addon_service.logo : current_page.data.default_logo %>" height="64" alt="<%= addon_service.name %> logo"></div>
                    <div class="col col-middle h6 em-high type-neutral-1">
                      <%= addon_service.name %>
                    </div>

                    <div class="col col-fixed col-bottom">
                      <% if addon_service.is_partner? %>
                        <div class="type-accent-5 txt-b h6">PARTNER</div>
                      <% else %>
                        <div class="type-brand-8 txt-b h6">VMware</div>
                      <% end %>
                    </div>
                  </div>
                </a>
              </div>
              <% end %>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>


    <%# ---------- OTHER SECTIONS -------------- %>

    <% current_page.data.misc_sections.each do |misc_section| %>

    <br />
    <h2 id="<%= misc_section.id ? misc_section.id : misc_section.name.downcase.strip.gsub(' ', '-').gsub(/[^\w-]/, '') %>">
      <%= misc_section.name %>
    </h2>

    <br />

    <% if misc_section.style == "text_only" %>
      <ul>

        <% misc_section.tiles.each do |link| %>
          <li class="h6 em-high type-neutral-1">
            <a href="<%= link.url %>"><%= link.name %></a>
          </li>
        <% end %>

      </ul>

    <% else%>

    <div class="grid">

      <% misc_section.tiles.each_with_index do |tile, index| %>
      <div class="col col-8 col-sm">

        <% if misc_section.style == "logo_only" %>
          <a class="docs-home-panel box-shadow-1 border-rounded col mbxl" href="<%= tile.url %>">
            <div class="grid docs-link-panel">
              <div class="logo-icon col-middle"><img src="images/<%= tile.logo? ? tile.logo : misc_section.default_logo? ? misc_section.default_logo : current_page.data.default_logo  %>" height="64" alt="<%= tile.name%>" title="<%= tile.name %>"></div>
            </div>
          </a>

        <% else %>
          <a class="docs-home-panel box-shadow-1 border-rounded col mbxl" href="<%= tile.url %>">
            <div class="grid docs-link-panel">
              <div class="col col-4 col-middle"><img src="images/<%= tile.logo? ? tile.logo : misc_section.default_logo? ? misc_section.default_logo : current_page.data.default_logo  %>" height="64" alt=""></div>
              <div class="col col-middle h6 em-high type-neutral-1">
                <%= tile.name %>
              </div>
            </div>
          </a>
        <% end %>
      </div>
      <% if index % 3 == 2 and index != 0 and index < misc_section.tiles.length - 1 %>
        </div>
        <div class="grid">
      <% end %>
    <% end %>

    </div>


    <% end %>
  <% end %>
  </div>
</div>
